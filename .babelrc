{
  "presets": [
    ["@babel/preset-env", {
      "modules": false,
      "targets": {
        "browsers": ["last 2 versions", "ie >= 10"]
      }
    }],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "transform-react-remove-prop-types"
  ],
  "env": {
    "esm": {
      "presets": [
        ["@babel/preset-env", {
          "modules": false
        }],
        "@babel/preset-react"
      ]
    }
  }
}
